{
  "name": "milind/php-pubsub-google-cloud",
  "description": "A Google Cloud adapter for the php-pubsub package",
  "license": "MIT",
  "authors": [
    {
      "name": "Milind",
      "email": "milind@getflits.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "milind/php-pubsub": "dev-master",
    "google/cloud-pubsub": "^1.31"
  },
  "autoload": {
    "psr-4": {
      "milind\\PubSub\\GoogleCloud\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^0.9.5"
  }
}
